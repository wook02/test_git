print("===============================================================")
print("                       구구단 표 (2단~9단) 출력")
print("===============================================================")

for dan in range(2, 10):
    print("#  {0}단".format(dan), end="\t")
print()


for i in range(1, 10):
    for dan in range(2, 10):
        print("{0}X{1}={2:2}".format(dan, i, dan * i), end="\t")
    print()